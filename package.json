{
  "name": "@softwareventures/nullable",
  "version": "0.0.0-development",
  "description": "Pure functional utilities for nullable types",
  "keywords": [
    "TypeScript",
    "functional",
    "lambda",
    "nullable",
    "option"
  ],
  "author": "Daniel Cassidy <mail@danielcassidy.me.uk>",
  "homepage": "https://github.com/softwareventures/nullable",
  "bugs": "https://github.com/softwareventures/nullable/issues",
  "repository": "github:softwareventures/nullable",
  "license": "ISC",
  "scripts": {
    "fix": "tsc --noEmit && eslint . --fix && prettier --write .",
    "lint": "tsc --noEmit && eslint . && prettier --check .",
    "prepare": "tsc",
    "semantic-release": "semantic-release",
    "test": "ava"
  },
  "sideEffects": false,
  "engines": {
    "node": "^12 || ^14 || >=16"
  },
  "dependencies": {
    "tslib": "2.4.0"
  },
  "devDependencies": {
    "@softwareventures/eslint-config": "5.1.5",
    "@softwareventures/prettier-config": "3.0.0",
    "@softwareventures/semantic-release-config": "2.0.2",
    "@softwareventures/tsconfig": "5.1.1",
    "ava": "4.3.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.23.1",
    "prettier": "2.7.1",
    "semantic-release": "17.4.7",
    "ts-node": "10.9.1",
    "typescript": "4.8.3"
  },
  "eslintConfig": {
    "root": true,
    "extends": "@softwareventures"
  },
  "prettier": "@softwareventures/prettier-config",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register/files"
    ]
  },
  "release": {
    "extends": "@softwareventures/semantic-release-config"
  },
  "publishConfig": {
    "access": "public"
  }
}
